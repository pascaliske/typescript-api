{
  "name": "typescript-api",
  "description": "Skeleton project for easy extendable TypeScript APIs.",
  "version": "0.0.0",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascaliske.dev",
    "url": "https://pascaliske.dev"
  },
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "build": "rimraf dist && tsc --project tsconfig.json",
    "watch": "nodemon --exec \"yarn run build && yarn run start\" --watch src --ext ts",
    "lint": "tslint --project tsconfig.json --fix",
    "test": "ava dist/modules/*/*.test.js",
    "format": "prettier --write \"**/*.{ts,md}\"",
    "changelog": "standard-changelog --release-count 1"
  },
  "repository": {
    "url": "https://github.com/pascaliske/typescript-api.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/typescript-api/issues"
  },
  "private": true,
  "dependencies": {
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.10",
    "routing-controllers": "^0.9.0",
    "socket-controllers": "^0.0.5",
    "socket.io": "^4.1.2",
    "tslib": "^2.3.0",
    "typedi": "0.8.0",
    "typeorm": "^0.2.34",
    "typeorm-routing-controllers-extensions": "^0.2.0",
    "typeorm-typedi-extensions": "^0.4.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/config": "^0.0.38",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^15.6.1",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.11",
    "ava": "^3.15.0",
    "chalk": "^4.1.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sql.js": "^1.5.0",
    "standard-changelog": "^2.0.27",
    "supertest": "^6.1.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.22.4",
    "typescript": "^4.3.5"
  }
}
